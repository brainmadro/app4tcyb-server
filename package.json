  },
  "dependencies": {
    "dotenv": "16.0.1",
    "ejs": "3.1.5",
    "express": "4.15.2"
  },
  "devDependencies": {
    "got": "11.3.0",
    "jest": "28.1.3",
    "nodemon": "2.0.21",
    "supertest": "6.2.4",
    "tape": "4.7.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
